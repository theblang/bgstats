{
  "name": "tkit",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "4.0.2",
    "@material-ui/icons": "4.0.1",
    "localforage": "1.7.3",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-router-dom": "5.0.1",
    "react-scripts": "3.0.1",
    "uuid": "3.3.2",
